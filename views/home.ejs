<%- include('partials/header') %>
    <h1>Home</h1>
    <p>
        <%= homeContent %>
    </p>
    <% posts.forEach((ele)=> { %>
        <h1>
            <%= ele.title %>
        </h1>

        <p>
            <%= ele.post.slice(0, 100) + " ..." %> 
            <a href="/posts/<%=ele.title %>">Read More</a>
        </p>

        <%}) %>
            <%- include('partials/footer') %>